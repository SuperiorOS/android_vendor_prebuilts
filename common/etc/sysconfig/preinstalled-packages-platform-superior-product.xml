<!-- System packages to preinstall on all devices with aosp_product, per user type.
     Documentation at frameworks/base/data/etc/preinstalled-packages-platform.xml
-->
<config>
    <!-- Wallpaperpicker -->
    <install-in-user-type package="com.android.wallpaperpicker">
        <install-in user-type="FULL" />
    </install-in-user-type>
    <!-- Messages -->
    <install-in-user-type package="com.android.messaging">
        <install-in user-type="FULL" />
        <install-in user-type="PROFILE" />
        <do-not-install-in user-type="android.os.usertype.profile.CLONE" />
    </install-in-user-type>
    <!-- Etar (Calendar) -->
    <install-in-user-type package="org.lineageos.etar">
        <install-in user-type="FULL" />
        <install-in user-type="PROFILE" />
        <do-not-install-in user-type="android.os.usertype.profile.CLONE" />
    </install-in-user-type>
    <!-- ExactCalculator -->
    <install-in-user-type package="com.android.calculator2">
        <install-in user-type="FULL" />
        <install-in user-type="PROFILE" />
        <do-not-install-in user-type="android.os.usertype.profile.CLONE" />
    </install-in-user-type>
    <!-- Aperture (Camera) -->
    <install-in-user-type package="org.lineageos.aperture">
        <install-in user-type="FULL" />
        <install-in user-type="PROFILE" />
        <do-not-install-in user-type="android.os.usertype.profile.CLONE" />
    </install-in-user-type>
    <!-- Chromium -->
    <install-in-user-type package="org.chromium.chrome">
        <install-in user-type="FULL" />
        <install-in user-type="PROFILE" />
        <do-not-install-in user-type="android.os.usertype.profile.CLONE" />
    </install-in-user-type>
    <!-- FM radio -->
    <install-in-user-type package="com.caf.fmradio">
        <install-in user-type="FULL" />
        <install-in user-type="PROFILE" />
        <do-not-install-in user-type="android.os.usertype.profile.CLONE" />
    </install-in-user-type>
    <!-- Documents UI -->
    <install-in-user-type package="com.android.documentsui">
        <install-in user-type="FULL" />
        <install-in user-type="PROFILE" />
        <do-not-install-in user-type="android.os.usertype.profile.CLONE" />
    </install-in-user-type>
    <!-- Launcher 3 -->
    <install-in-user-type package="com.android.launcher3">
        <!-- Unfortunately some things (eg recents) look specifically for launcher in user 0 -->
        <install-in user-type="SYSTEM" />
        <install-in user-type="FULL" />
        <do-not-install-in user-type="android.os.usertype.profile.CLONE" />
    </install-in-user-type>
    <!-- Data restore -->
    <install-in-user-type package="com.google.android.apps.restore">
        <install-in user-type="FULL" />
        <install-in user-type="PROFILE" />
        <do-not-install-in user-type="android.os.usertype.profile.CLONE" />
    </install-in-user-type>
    <!-- Google assistant -->
    <install-in-user-type package="com.google.android.apps.googleassistant">
        <install-in user-type="FULL" />
        <install-in user-type="PROFILE" />
        <do-not-install-in user-type="android.os.usertype.profile.CLONE" />
    </install-in-user-type>
    <!-- Google EUICC -->
    <install-in-user-type package="com.google.android.euicc">
        <install-in user-type="FULL" />
        <install-in user-type="PROFILE" />
        <do-not-install-in user-type="android.os.usertype.profile.CLONE" />
    </install-in-user-type>
    <!-- STK -->
    <install-in-user-type package="com.android.stk">
        <install-in user-type="FULL" />
        <install-in user-type="PROFILE" />
        <do-not-install-in user-type="android.os.usertype.profile.CLONE" />
    </install-in-user-type>
    <!-- Cellbroadcaster -->
    <install-in-user-type package="com.android.cellbroadcastreceiver.module">
        <install-in user-type="FULL" />
        <install-in user-type="PROFILE" />
        <do-not-install-in user-type="android.os.usertype.profile.CLONE" />
    </install-in-user-type>
    <!-- Via -->
    <install-in-user-type package="mark.via.gp">
        <install-in user-type="FULL" />
        <install-in user-type="PROFILE" />
        <do-not-install-in user-type="android.os.usertype.profile.CLONE" />
    </install-in-user-type>
    <!-- MIUI Gallery -->
    <install-in-user-type package="com.miui.gallery">
        <install-in user-type="FULL" />
        <install-in user-type="PROFILE" />
        <do-not-install-in user-type="android.os.usertype.profile.CLONE" />
    </install-in-user-type>
    <!-- Dot Gallery -->
    <install-in-user-type package="com.dot.gallery">
        <install-in user-type="FULL" />
        <install-in user-type="PROFILE" />
        <do-not-install-in user-type="android.os.usertype.profile.CLONE" />
    </install-in-user-type>
    <!-- Google Keyboard -->
    <install-in-user-type package="com.google.android.inputmethod.latin">
        <install-in user-type="FULL" />
        <install-in user-type="PROFILE" />
        <do-not-install-in user-type="android.os.usertype.profile.CLONE" />
    </install-in-user-type>
    <!-- Android Auto -->
    <install-in-user-type package="com.google.android.projection.gearhead">
        <install-in user-type="FULL" />
        <install-in user-type="PROFILE" />
        <do-not-install-in user-type="android.os.usertype.profile.CLONE" />
    </install-in-user-type>
    <!-- Android Accessibility -->
    <install-in-user-type package="com.android.systemui.accessibility.accessibilitymenu">
        <install-in user-type="FULL" />
        <install-in user-type="PROFILE" />
        <do-not-install-in user-type="android.os.usertype.profile.CLONE" />
    </install-in-user-type>
    <!-- Play Store -->
    <install-in-user-type package="com.android.vending">
        <install-in user-type="FULL" />
        <install-in user-type="PROFILE" />
        <do-not-install-in user-type="android.os.usertype.profile.CLONE" />
    </install-in-user-type>
    <!-- Google -->
    <install-in-user-type package="com.google.android.googlequicksearchbox">
        <install-in user-type="FULL" />
        <install-in user-type="PROFILE" />
        <do-not-install-in user-type="android.os.usertype.profile.CLONE" />
    </install-in-user-type>
    <!-- Digital wellbing -->
    <install-in-user-type package="com.google.android.apps.wellbing">
        <install-in user-type="FULL" />
        <install-in user-type="PROFILE" />
        <do-not-install-in user-type="android.os.usertype.profile.CLONE" />
    </install-in-user-type>
    <!-- GameSpace -->
    <install-in-user-type package="io.chaldeaprjkt.gamespace">
        <install-in user-type="FULL" />
        <install-in user-type="PROFILE" />
        <do-not-install-in user-type="android.os.usertype.profile.CLONE" />
    </install-in-user-type>
    <!-- Dolby -->
    <install-in-user-type package="com.dolby.daxappui">
        <install-in user-type="FULL" />
        <install-in user-type="PROFILE" />
        <do-not-install-in user-type="android.os.usertype.profile.CLONE" />
    </install-in-user-type>
</config>
